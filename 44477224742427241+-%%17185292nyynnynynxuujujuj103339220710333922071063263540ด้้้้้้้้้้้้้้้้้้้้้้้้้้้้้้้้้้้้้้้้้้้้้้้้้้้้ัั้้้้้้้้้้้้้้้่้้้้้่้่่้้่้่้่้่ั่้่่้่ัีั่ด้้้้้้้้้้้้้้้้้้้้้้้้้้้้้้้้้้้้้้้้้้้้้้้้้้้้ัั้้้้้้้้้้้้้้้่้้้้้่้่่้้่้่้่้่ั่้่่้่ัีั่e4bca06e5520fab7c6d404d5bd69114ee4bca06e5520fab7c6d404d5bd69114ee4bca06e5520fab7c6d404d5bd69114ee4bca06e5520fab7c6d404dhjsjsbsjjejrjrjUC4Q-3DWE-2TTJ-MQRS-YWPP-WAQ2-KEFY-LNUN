-- 加载UI库
local repo = "https://raw.githubusercontent.com/ATLASTEAM01/Obsidian/main/"
local Library = loadstring(game:HttpGet(repo .. "Library.lua"))()
local ThemeManager = loadstring(game:HttpGet(repo .. "addons/ThemeManager.lua"))()
local SaveManager = loadstring(game:HttpGet(repo .. "addons/SaveManager.lua"))()

-- 等待游戏加载
if not game:IsLoaded() then
    game.Loaded:Wait()
end

-- 初始化设置
local TranslationSettings = {
    Enabled = false,
    AutoTranslate = true,
    TranslateSpeed = 0.5,
    TargetLanguage = "英→中",
    ShowCurrentTime = true,
    ShowPlayerName = true,
    ShowNewYearCountdown = true,
    ShowExecutorInfo = true,
    ShowServerID = true,
    TranslationLinkCopied = false,
    NotificationDuration = 3,
    TranslationCount = 0
}

-- 游戏服务
local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local HttpService = game:GetService("HttpService")
local TextService = game:GetService("TextService")
local TweenService = game:GetService("TweenService")
local LocalPlayer = Players.LocalPlayer
local CoreGui = game:GetService("CoreGui")

-- 全局变量
local translationCache = {}
local lastTranslationTime = 0
local currentTranslations = {}
local translationCounter = 0
local lastFullTranslationTime = 0

-- 创建通知GUI
local NotificationGui = Instance.new("ScreenGui")
NotificationGui.Name = "TranslationNotifications"
NotificationGui.ResetOnSpawn = false
NotificationGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
NotificationGui.Parent = CoreGui

local function createNotification(title, message, duration)
    duration = duration or TranslationSettings.NotificationDuration
    
    -- 创建通知框架
    local notificationFrame = Instance.new("Frame")
    notificationFrame.Name = "Notification"
    notificationFrame.Size = UDim2.new(0, 320, 0, 80)
    notificationFrame.Position = UDim2.new(1, -340, 0, 20)
    notificationFrame.BackgroundColor3 = Color3.fromRGB(25, 25, 35)
    notificationFrame.BackgroundTransparency = 0.2
    notificationFrame.BorderSizePixel = 0
    notificationFrame.ZIndex = 10000
    
    -- 圆角
    local corner = Instance.new("UICorner")
    corner.CornerRadius = UDim.new(0, 10)
    corner.Parent = notificationFrame
    
    -- 边框
    local stroke = Instance.new("UIStroke")
    stroke.Color = Color3.fromRGB(100, 100, 200)
    stroke.Thickness = 2
    stroke.Parent = notificationFrame
    
    -- 标题
    local titleLabel = Instance.new("TextLabel")
    titleLabel.Name = "Title"
    titleLabel.Size = UDim2.new(1, -20, 0, 25)
    titleLabel.Position = UDim2.new(0, 10, 0, 5)
    titleLabel.BackgroundTransparency = 1
    titleLabel.Text = title
    titleLabel.TextColor3 = Color3.fromRGB(255, 100, 100)
    titleLabel.TextSize = 16
    titleLabel.Font = Enum.Font.GothamBold
    titleLabel.TextXAlignment = Enum.TextXAlignment.Left
    titleLabel.TextYAlignment = Enum.TextYAlignment.Center
    titleLabel.ZIndex = 10001
    titleLabel.Parent = notificationFrame
    
    -- 消息
    local messageLabel = Instance.new("TextLabel")
    messageLabel.Name = "Message"
    messageLabel.Size = UDim2.new(1, -20, 0, 45)
    messageLabel.Position = UDim2.new(0, 10, 0, 30)
    messageLabel.BackgroundTransparency = 1
    messageLabel.Text = message
    messageLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
    messageLabel.TextSize = 14
    messageLabel.Font = Enum.Font.Gotham
    messageLabel.TextXAlignment = Enum.TextXAlignment.Left
    messageLabel.TextYAlignment = Enum.TextYAlignment.Top
    messageLabel.TextWrapped = true
    messageLabel.ZIndex = 10001
    messageLabel.Parent = notificationFrame
    
    notificationFrame.Parent = NotificationGui
    
    -- 将所有现有通知上移
    for _, child in ipairs(NotificationGui:GetChildren()) do
        if child:IsA("Frame") and child.Name == "Notification" then
            local currentY = child.Position.Y.Offset
            child.Position = UDim2.new(1, -340, 0, currentY + 90)
        end
    end
    
    -- 淡入动画
    notificationFrame.BackgroundTransparency = 1
    titleLabel.TextTransparency = 1
    messageLabel.TextTransparency = 1
    
    local fadeIn = TweenService:Create(notificationFrame, 
        TweenInfo.new(0.5, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),
        {BackgroundTransparency = 0.2})
    fadeIn:Play()
    
    local textFadeIn = TweenService:Create(titleLabel,
        TweenInfo.new(0.5, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),
        {TextTransparency = 0})
    textFadeIn:Play()
    
    local messageFadeIn = TweenService:Create(messageLabel,
        TweenInfo.new(0.5, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),
        {TextTransparency = 0})
    messageFadeIn:Play()
    
    -- 延迟后淡出
    task.delay(duration, function()
        local fadeOut = TweenService:Create(notificationFrame,
            TweenInfo.new(0.5, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),
            {BackgroundTransparency = 1, Position = UDim2.new(1, 400, notificationFrame.Position.Y.Scale, notificationFrame.Position.Y.Offset)})
        fadeOut:Play()
        
        local textFadeOut = TweenService:Create(titleLabel,
            TweenInfo.new(0.5, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),
            {TextTransparency = 1})
        textFadeOut:Play()
        
        local messageFadeOut = TweenService:Create(messageLabel,
            TweenInfo.new(0.5, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),
            {TextTransparency = 1})
        messageFadeOut:Play()
        
        -- 动画完成后移除
        fadeOut.Completed:Connect(function()
            notificationFrame:Destroy()
        end)
    end)
end

