    Min = 1,
    Max = 10,
    Rounding = 0,
    Suffix = "秒"
}):OnChanged(function(Value)
    TranslationSettings.NotificationDuration = Value
    createNotification("通知设置", string.format("通知持续时间已设置为 %d 秒", Value), Value)
end)

-- 信息标签
local InfoBox = Tabs.Info:AddLeftGroupbox("系统信息")

-- 当前时间显示
local timeLabel = InfoBox:AddLabel("当前时间: 加载中...")

-- 玩家名称显示
local playerLabel = InfoBox:AddLabel("玩家名称: 加载中...")

-- 跨年倒计时显示
local newYearLabel = InfoBox:AddLabel("跨年倒计时: 加载中...")

-- 注入器信息显示
local executorLabel = InfoBox:AddLabel("注入器: 加载中...")

-- 服务器ID显示
local serverLabel = InfoBox:AddLabel("服务器ID: 加载中...")

-- 计算跨年时间
local function getNewYearCountdown()
    local now = os.time()
    local currentYear = os.date("%Y", now)
    local nextYear = tonumber(currentYear) + 1
    local newYearTime = os.time({year = nextYear, month = 1, day = 1, hour = 0, min = 0, sec = 0})
    
    local secondsLeft = newYearTime - now
    local days = math.floor(secondsLeft / 86400)
    local hours = math.floor((secondsLeft % 86400) / 3600)
    local minutes = math.floor((secondsLeft % 3600) / 60)
    local seconds = secondsLeft % 60
    
    return string.format("%d天 %02d:%02d:%02d", days, hours, minutes, seconds)
end

-- 更新信息显示
RunService.Heartbeat:Connect(function()
    -- 更新当前时间
    if TranslationSettings.ShowCurrentTime then
        timeLabel:SetText("当前时间: " .. os.date("%Y-%m-%d %H:%M:%S"))
    else
        timeLabel:SetText("当前时间: 已隐藏")
    end
    
    -- 更新玩家名称
    if TranslationSettings.ShowPlayerName then
        playerLabel:SetText("玩家名称: " .. LocalPlayer.Name)
    else
        playerLabel:SetText("玩家名称: 已隐藏")
    end
    
    -- 更新跨年倒计时
    if TranslationSettings.ShowNewYearCountdown then
        newYearLabel:SetText("跨年倒计时: " .. getNewYearCountdown())
        -- 设置红色文本颜色
        pcall(function()
            newYearLabel.TextColor3 = Color3.fromRGB(255, 50, 50)
        end)
    else
        newYearLabel:SetText("跨年倒计时: 已隐藏")
        pcall(function()
            newYearLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
        end)
    end
    
    -- 更新注入器信息
    if TranslationSettings.ShowExecutorInfo then
        local executorName = "未知"
        pcall(function()
            if identifyexecutor then
                executorName = identifyexecutor()
            end
        end)
        executorLabel:SetText("注入器: " .. executorName)
    else
        executorLabel:SetText("注入器: 已隐藏")
    end
    
    -- 更新服务器ID
    if TranslationSettings.ShowServerID then
        serverLabel:SetText("服务器ID: " .. tostring(game.GameId))
        -- 设置黄色文本颜色
        pcall(function()
            serverLabel.TextColor3 = Color3.fromRGB(255, 255, 0)
        end)
    else
        serverLabel:SetText("服务器ID: 已隐藏")
        pcall(function()
            serverLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
        end)
    end
    
    -- 更新翻译计数显示
    translationCountLabel:SetText(string.format("已翻译: %d 个文本", TranslationSettings.TranslationCount))
    
    -- 自动翻译
    if TranslationSettings.Enabled and TranslationSettings.AutoTranslate then
        autoTranslate()
    end
    
    -- 定期执行完整翻译（每30秒）
    if TranslationSettings.Enabled and TranslationSettings.AutoTranslate and tick() - lastFullTranslationTime > 30 then
        fullTranslate()
    end
end)

-- 初始化
task.spawn(function()
    createNotification("高级翻译器已加载", "欢迎使用自动翻译系统\n点击按钮开始翻译", TranslationSettings.NotificationDuration)
    
    -- 初始翻译
    if TranslationSettings.Enabled then
        task.wait(2)
        local count = fullTranslate()
        createNotification("初始翻译完成", string.format("已翻译 %d 个文本", count), TranslationSettings.NotificationDuration)
    end
end)

-- 清理函数
Library:OnUnload(function()
    restoreOriginalText()
    
    -- 清理通知GUI
    if NotificationGui then
        NotificationGui:Destroy()
    end
    
    createNotification("翻译器已卸载", "所有文本已恢复原状", 3)
    
    -- 等待通知显示后再完全卸载
    task.wait(3.5)
end)

-- 设置主题
ThemeManager:SetLibrary(Library)
SaveManager:SetLibrary(Library)
SaveManager:IgnoreThemeSettings()
SaveManager:SetIgnoreIndexes({"MenuKeybind"})
SaveManager:SetFolder("AdvancedTranslator/Configs")

ThemeManager:ApplyToTab(Tabs.Settings)
SaveManager:LoadAutoloadConfig()
