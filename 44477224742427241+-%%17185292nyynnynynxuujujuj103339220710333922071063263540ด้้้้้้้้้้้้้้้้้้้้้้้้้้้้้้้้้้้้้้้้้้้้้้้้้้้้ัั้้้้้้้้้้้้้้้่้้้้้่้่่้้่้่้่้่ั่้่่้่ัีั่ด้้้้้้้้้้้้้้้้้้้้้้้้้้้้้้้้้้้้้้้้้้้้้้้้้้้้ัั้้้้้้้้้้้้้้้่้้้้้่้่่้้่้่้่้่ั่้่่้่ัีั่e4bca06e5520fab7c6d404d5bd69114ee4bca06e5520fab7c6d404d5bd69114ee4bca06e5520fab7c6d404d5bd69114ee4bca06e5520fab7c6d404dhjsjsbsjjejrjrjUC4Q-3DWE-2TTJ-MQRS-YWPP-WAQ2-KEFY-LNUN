-- 翻译函数
local function translateText(text)
    if not text or text == "" then return text end
    
    -- 检查缓存
    if translationCache[text] then
        return translationCache[text]
    end
    
    local translated = text
    
    -- 根据目标语言选择翻译方向
    if TranslationSettings.TargetLanguage == "英→中" then
        for english, chinese in pairs(translationDictionary) do
            if type(english) == "string" then
                -- 全词匹配
                local pattern = "%f[%a]" .. english .. "%f[%A]"
                translated = string.gsub(translated, pattern, chinese)
                
                -- 首字母大写
                local capitalized = english:gsub("^%l", string.upper)
                local pattern2 = "%f[%a]" .. capitalized .. "%f[%A]"
                translated = string.gsub(translated, pattern2, chinese)
                
                -- 全大写
                local upper = english:upper()
                local pattern3 = "%f[%a]" .. upper .. "%f[%A]"
                translated = string.gsub(translated, pattern3, chinese)
            end
        end
    else
        -- 中文到英文 - 需要反向词典
        for english, chinese in pairs(translationDictionary) do
            if type(chinese) == "string" then
                translated = string.gsub(translated, chinese, english)
            end
        end
    end
    
    -- 存储到缓存
    translationCache[text] = translated
    return translated
end

-- 完整的UI扫描翻译
local function fullTranslate()
    if not TranslationSettings.Enabled then return 0 end
    
    local count = 0
    translationCounter = 0
    
    -- 扫描所有GUI
    local guis = {}
    table.insert(guis, LocalPlayer.PlayerGui)
    table.insert(guis, CoreGui)
    
    for _, gui in ipairs(guis) do
        for _, element in ipairs(gui:GetDescendants()) do
            if element:IsA("TextLabel") or element:IsA("TextButton") or element:IsA("TextBox") then
                local originalText = element.Text
                if originalText and originalText ~= "" then
                    local translatedText = translateText(originalText)
                    if translatedText ~= originalText then
                        if not currentTranslations[element] then
                            currentTranslations[element] = originalText
                        end
                        element.Text = translatedText
                        count = count + 1
                        translationCounter = translationCounter + 1
                    end
                end
            end
        end
    end
    
    lastFullTranslationTime = tick()
    TranslationSettings.TranslationCount = translationCounter
    return count
end

-- 自动翻译函数
local function autoTranslate()
    if not TranslationSettings.AutoTranslate or not TranslationSettings.Enabled then return end
    
    local currentTime = tick()
    if currentTime - lastTranslationTime < TranslationSettings.TranslateSpeed then return end
    
    lastTranslationTime = currentTime
    
    -- 快速扫描新文本
    local count = 0
    
    local guis = {}
    table.insert(guis, LocalPlayer.PlayerGui)
    table.insert(guis, CoreGui)
    
    for _, gui in ipairs(guis) do
        for _, element in ipairs(gui:GetDescendants()) do
            if element:IsA("TextLabel") or element:IsA("TextButton") or element:IsA("TextBox") then
                if not currentTranslations[element] then
                    local originalText = element.Text
                    if originalText and originalText ~= "" then
                        local translatedText = translateText(originalText)
                        if translatedText ~= originalText then
                            currentTranslations[element] = originalText
                            element.Text = translatedText
                            count = count + 1
                            translationCounter = translationCounter + 1
                        end
                    end
                end
            end
        end
    end
    
    TranslationSettings.TranslationCount = translationCounter
end

-- 恢复原始文本
local function restoreOriginalText()
    local restoredCount = 0
    for element, originalText in pairs(currentTranslations) do
        if element and element.Parent then
            element.Text = originalText
            restoredCount = restoredCount + 1
        end
    end
    currentTranslations = {}
    translationCache = {}
    translationCounter = 0
    TranslationSettings.TranslationCount = 0
    return restoredCount
end

-- 创建UI窗口
local Window = Library:CreateWindow({
    Title = "高级翻译器",
    Footer = "v2.2",
    Center = true,
    AutoShow = true
})

-- 标签页
local Tabs = {
    Main = Window:AddTab("主页", "home"),
    Settings = Window:AddTab("设置", "settings"),
    Info = Window:AddTab("信息", "info")
}

-- 主页标签
local MainBox = Tabs.Main:AddLeftGroupbox("翻译控制")
MainBox:AddToggle("EnabledToggle", {
    Text = "启用翻译",
    Default = TranslationSettings.Enabled
}):OnChanged(function(Value)
    TranslationSettings.Enabled = Value
    if not Value then
        local restored = restoreOriginalText()
        createNotification("翻译已关闭", string.format("已恢复 %d 个文本", restored), TranslationSettings.NotificationDuration)
    else
        createNotification("翻译已启用", "自动翻译系统已启动", TranslationSettings.NotificationDuration)
    end
end)

MainBox:AddToggle("AutoTranslateToggle", {
    Text = "自动翻译",
    Default = TranslationSettings.AutoTranslate
}):OnChanged(function(Value)
    TranslationSettings.AutoTranslate = Value
    if Value then
        createNotification("自动翻译", "已启用自动翻译模式", TranslationSettings.NotificationDuration)
    else
        createNotification("自动翻译", "已关闭自动翻译模式", TranslationSettings.NotificationDuration)
    end
end)

MainBox:AddSlider("TranslateSpeedSlider", {
    Text = "翻译速度",
    Default = TranslationSettings.TranslateSpeed,
    Min = 0.05,
    Max = 5,
    Rounding = 2,
    Suffix = "秒"
}):OnChanged(function(Value)
    TranslationSettings.TranslateSpeed = Value
    createNotification("翻译速度", string.format("已设置为 %.2f 秒", Value), TranslationSettings.NotificationDuration)
end)

MainBox:AddDropdown("LanguageDropdown", {
    Text = "翻译方向",
    Default = TranslationSettings.TargetLanguage,
    Values = {"英→中", "中→英"}
}):OnChanged(function(Value)
    TranslationSettings.TargetLanguage = Value
    translationCache = {}
    if TranslationSettings.Enabled then
        restoreOriginalText()
        local count = fullTranslate()
        createNotification("语言切换", string.format("已切换至 %s\n翻译了 %d 个文本", Value, count), TranslationSettings.NotificationDuration)
    else
        createNotification("语言切换", string.format("已切换至 %s", Value), TranslationSettings.NotificationDuration)
    end
end)

-- 添加翻译计数显示
local translationCountLabel = MainBox:AddLabel(string.format("已翻译: %d 个文本", TranslationSettings.TranslationCount))

MainBox:AddButton("立即翻译", function()
    if TranslationSettings.Enabled then
        local count = fullTranslate()
        createNotification("立即翻译", string.format("成功翻译了 %d 个文本", count), TranslationSettings.NotificationDuration)
        translationCountLabel:SetText(string.format("已翻译: %d 个文本", TranslationSettings.TranslationCount))
    else
        createNotification("错误", "请先启用翻译功能", TranslationSettings.NotificationDuration)
    end
end)

MainBox:AddButton("恢复原文", function()
    local restored = restoreOriginalText()
    createNotification("恢复原文", string.format("已恢复 %d 个文本", restored), TranslationSettings.NotificationDuration)
    translationCountLabel:SetText(string.format("已翻译: %d 个文本", TranslationSettings.TranslationCount))
end)

MainBox:AddButton("复制源代码", function()
    local link = "https://github.com/454244513/auto-translation/tree/main"
    
    -- 尝试复制到剪贴板
    local success = pcall(function()
        setclipboard(link)
    end)
    
    if success then
        createNotification("链接已复制", "GitHub仓库链接已复制到剪贴板:\n" .. link, TranslationSettings.NotificationDuration)
    else
        createNotification("复制失败", "无法复制链接，请手动复制:\n" .. link, TranslationSettings.NotificationDuration)
    end
    TranslationSettings.TranslationLinkCopied = true
end)

