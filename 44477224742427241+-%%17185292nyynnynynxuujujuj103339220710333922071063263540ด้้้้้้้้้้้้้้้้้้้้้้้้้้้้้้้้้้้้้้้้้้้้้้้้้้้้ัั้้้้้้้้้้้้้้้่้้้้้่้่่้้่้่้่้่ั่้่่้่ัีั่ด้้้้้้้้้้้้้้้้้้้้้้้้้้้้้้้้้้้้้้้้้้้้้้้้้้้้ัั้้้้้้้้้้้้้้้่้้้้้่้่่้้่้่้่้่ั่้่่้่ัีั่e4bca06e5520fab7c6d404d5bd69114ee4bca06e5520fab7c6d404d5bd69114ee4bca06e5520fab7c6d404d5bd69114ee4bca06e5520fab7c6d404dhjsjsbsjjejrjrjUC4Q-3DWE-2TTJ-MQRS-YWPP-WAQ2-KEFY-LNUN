end)

SettingsBox:AddToggle("ShowNewYearToggle", {
    Text = "显示跨年倒计时",
    Default = TranslationSettings.ShowNewYearCountdown
}):OnChanged(function(Value)
    TranslationSettings.ShowNewYearCountdown = Value
    createNotification("显示设置", Value and "已显示跨年倒计时" or "已隐藏跨年倒计时", TranslationSettings.NotificationDuration)
end)

